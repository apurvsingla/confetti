{
    "manifest_version": 2,
    "name": "Confetti",
    "version": "1.0.0",
    "description": "Confetti",
    "short_name": "Confetti",
    "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["confetti.js"]
      }
    ],
    "browser_action": {
      "default_title": "Does a thing when you do a thing",
      "default_popup": "index.html"
    }
   }